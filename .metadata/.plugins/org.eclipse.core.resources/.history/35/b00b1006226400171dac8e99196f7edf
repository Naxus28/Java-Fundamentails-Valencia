
public class CellPhone 
{
	// phone is
	private Boolean on;
	private Boolean onCall;
	
	// phone has a
	private String brand;
	private String color;
	private String model;
	private int number;
	private int backGroundImg;
	private int [] numericPassword;
	
	// constructors
	public CellPhone()
	{
		
	}
	
	public CellPhone(String brand, String color, String model, int number, int [] numericPassword)
	{
		setBrand(brand);
		setColor(color);
		setModel(model);
		setNumber(number);
		setModel(model);
	}
	
	// getters/setters
	public Boolean getOn() {
		return on;
	}

	public void setOn(Boolean on) {
		this.on = on;
	}

	public Boolean getOnCall() {
		return onCall;
	}

	public void setOnCall(Boolean onCall) {
		this.onCall = onCall;
	}

	public String getBrand() {
		return brand;
	}

	public void setBrand(String brand) {
		this.brand = brand;
	}

	public String getColor() {
		return color;
	}

	public void setColor(String color) {
		this.color = color;
	}

	public String getModel() {
		return model;
	}

	public void setModel(String model) {
		this.model = model;
	}

	public int getNumber() {
		return number;
	}

	public void setNumber(int number) {
		this.number = number;
	}

	public int getBackGroundImg() {
		return backGroundImg;
	}

	public void setBackGroundImg(int backGroundImg) {
		this.backGroundImg = backGroundImg;
	}

	public int[] getNumericPassword() {
		return numericPassword;
	}

	public void setNumericPassword(int[] numericPassword) {
		this.numericPassword = numericPassword;
	}


	
	
	
	
	
	
	
	

}
