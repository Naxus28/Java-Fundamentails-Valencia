
public class RectangleTest {
	
	public static void printLineBreak(String text)
	{
		String textToPrint = Boolean.getBoolean(text) ? "==========" + text + "==========\n" : "====================\n";
		System.out.println();
		System.out.println(textToPrint);
	}
	
	public static void main(String[] args) 
	{
		
		
		
		Rectangle parentRectangle = new Rectangle();
		System.out.println(parentRectangle);
		System.out.println();
		System.out.println("==========Re-setting parent rectangle values==========\n");
		parentRectangle.setWidth(13);
		parentRectangle.setHeight(30);
	
		System.out.println(parentRectangle);
		
		
		
		
		Rectangle rect1 = new Rectangle("Rect Child One", 10, 20);
		System.out.println(rect1);

	}

}
