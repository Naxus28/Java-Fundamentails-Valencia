
public class ExceptionsVideoExamples {

	public static void main(String[] args) {
		exceptionOne();
		
	}
	
	/**
	 * first exception
	 */
	public static void exceptionOne() {
		int a, b = 6;
		 
		 try {
			 a = b / 0;
		 } catch(ArithmeticException ae) {
			 System.out.println("Arithmetic Exception: " + ae.getMessage());
		 }
	}

}
