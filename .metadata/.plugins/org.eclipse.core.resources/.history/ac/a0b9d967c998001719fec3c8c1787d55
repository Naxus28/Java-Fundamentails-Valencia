
public class OverridingStaticMethod {
	static String name = getName(); // since this subclass has a 
	static String parentName = Overriding.getName(); // invokes suerclass "getName()" directly (cannot use "super" for static methods)
	
	// overrides parent class' static "getName" method
	public static String getName() {
		return "Gabriel in subclass";
	}

	public static void main(String[] args) {
		System.out.println(name);
		System.out.println(parentName); 
	}

}
