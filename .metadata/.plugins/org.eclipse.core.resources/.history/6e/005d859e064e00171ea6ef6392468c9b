 * 
 */

/**
 * @author gabrielferraz
 *
 */
public class NestedLoop {

	public static void main(String[] args) {
		
		test:
		for(int i = 0; i < 3; i++) 
		{
			int j = 0;
			System.out.println("Outer loop: " + i);
			
			while(true)
			{
				System.out.println("Inner loop: " + j);
				
				
				if(j > 2) 
				{
					break test;
				}
				j++;
			}
			
		}
		
		System.out.println("Outside both loops");

	}

}
