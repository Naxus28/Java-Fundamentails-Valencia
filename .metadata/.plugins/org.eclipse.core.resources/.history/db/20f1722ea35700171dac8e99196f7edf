/**
 * 
 */

/**
 * @author gabrielferraz
 *
 */
public class Doubler {
	
	static int index = 0;

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Doubler cm = new Doubler();
		cm.callClassMethod();
	}
	
	private void callClassMethod()
	{	
		System.out.println("index is: " + index);
		index++;
		
		if(index < 10) 
		{
			this.callClassMethod();
		} 
	}

}
